{
  "name": "oas-telemetry",
  "version": "0.1.0",
  "description": "This package export an express midelware that allows to trace the requests and responses of an express application using OpenTelemetry",
  "author": "Manuel Otero",
  "contributors": ["Alejandro Santisteban"],
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "babel src -d dist --out-file-extension .cjs",
    "publish": "npm run build && npm publish"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.cjs",
  "module": "./src/index.js",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./src/index.js",
    "default": "./src/index.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "nedb": "^1.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.24.4",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.43.0",
    "@opentelemetry/sdk-node": "^0.49.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-module-extension": "^0.1.3",
    "nodemon": "^3.1.0"
  }
}
